{
  "name": "FU Connect",
  "version": "1.3.0",
  "description": "Auto connect to dormitory network for FPT University Student!",
  "manifest_version": 2,
  "icons": { 
              "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png" 
            },
  "browser_action": {
   "default_icon": "icon48.png",
   "default_title": "FU Connect",
   "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
        "storage"
    ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "tabs",
    "storage",
    "http://*/*", 
    "https://*/*"
  ]
}
